    "def get_changepoint_matrix(T, S):\n",
    "    step = T/(S+1)\n",
    "    s = jnp.arange(start=0, stop=T, step=step).astype(int)[1:]\n",
    "    f = jax.vmap(lambda x: jnp.where(jnp.arange(T) >= x, jnp.ones(T), jnp.zeros(T)), 0, 0)\n",
    "    A = f(s).T\n",
    "    return A, s\n",
    "\n",
    "print(get_changepoint_matrix(10, 2))"
   ]
  },
